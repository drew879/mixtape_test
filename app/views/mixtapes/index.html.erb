<h2>Mixtapes</h2>

<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>Actions</th>
  </tr>

  <% @mixtapes.each do |mixtape| %>
    <tr>
      <td><%= link_to mixtape.name, mixtape %></td>
      <td>
        <%= link_to 'Edit', edit_mixtape_path(mixtape), :class => "btn btn-mini" %>
        <%= link_to 'Destroy', mixtape, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-mini btn-danger" %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Mixtape', new_mixtape_path, :class => "btn btn-primary" %>
